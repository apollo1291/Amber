{
"manifest_version": 3,
"name": "Amber",
"Description": "Track Carbon Emissions",
"version": "1.0",

"content_scripts": [{
    "matches": ["https://www.amazon.com/*"],
    "js": ["Scrapers/scrapeAmazon.js", "estimateCO2.js", "UI/amazonDisplay.js"],
    "css": ["UI/popup.css"],
    "run_at": "document_end"
},
{
    "matches": ["https://www.ebay.com/*"],
    "js": [],
    "css":["UI/popup.css"],
    "run_at": "document_end"
}
]


}
